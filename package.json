{
  "name": "equation-parser",
  "version": "0.0.0",
  "description": "Parse equations to an AST",
  "main": "dist/index.js",
  "repository": "git@github.com:kgram/equation-parser.git",
  "author": "Kristoffer Gram <kristoffer.gram@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "eslint \"src/**/*.ts\"",
    "build:types": "tsc -d --emitDeclarationOnly --outDir decl",
    "build:js": "rollup"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.7.11",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "eslint": "^6.6.0",
    "jest": "^24.8.0",
    "rollup": "^1.26.3",
    "rollup-plugin-multi-input": "^1.0.2",
    "typescript": "^3.5.3"
  },
  "babel": {
    "presets": [
      "@babel/typescript",
      "@babel/env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "jest": {
    "transform": {
      "\\.(js|ts|tsx)$": "babel-jest"
    },
    "testRegex": "(/tests/(?!helpers/).*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/tests/"
    ]
  }
}
